{
  "manifest_version": 3,
  "name": "RugProofAI",
  "version": "1.0.0",
  "description": "Scan your wallet for spam tokens and malicious contracts",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["https://*.covalenthq.com/"]
}
